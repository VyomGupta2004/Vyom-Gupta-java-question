import java.util.Scanner;
public class reverse_integer {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        System.out.print("INPUT :");
        int num=sc.nextInt();
        int rev=0;
        while(num>0) {
            int rem = num % 10;
            rev = rem + rev * 10;
            num = num / 10;
        }
        System.out.print("OUTPUT :");
        System.out.print(rev);
    }
}
